{
    "name": "saeedvir/supabase",
    "description": "A Laravel package for integrating with Supabase services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "saeedvir",
            "email": "saeedvirani@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Saeedvir\\Supabase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saeedvir\\Supabase\\SupabaseServiceProvider"
            ],
            "aliases": {
                "Supabase": "Saeedvir\\Supabase\\Facades\\Supabase"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}